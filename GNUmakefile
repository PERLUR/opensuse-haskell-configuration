all:: ghc-zot/ghc-zot.spec ghc-zot/zot-0.0.3.tar.gz

ghc-zot/zot-0.0.3.tar.gz:
	mkdir -p ghc-zot
	cd ghc-zot && wget -q http://hackage.haskell.org/package/zot-0.0.3/zot-0.0.3.tar.gz

ghc-zot/ghc-zot.spec:
	mkdir -p ghc-zot
	cd ghc-zot && rm -f *.spec && cblrpm spec zot-0.0.3
	spec-cleaner -i $@
