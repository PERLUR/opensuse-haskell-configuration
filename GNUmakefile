# GNUmakefile

.PHONY: all

all:: ghc-abstract-deque/ghc-abstract-deque.spec ghc-abstract-deque/abstract-deque-0.3.tar.gz

ghc-abstract-deque/abstract-deque-0.3.tar.gz:
	mkdir -p ghc-abstract-deque
	cd ghc-abstract-deque && wget -q http://hackage.haskell.org/package/abstract-deque-0.3/abstract-deque-0.3.tar.gz

ghc-abstract-deque/ghc-abstract-deque.spec:
	mkdir -p ghc-abstract-deque
	cd ghc-abstract-deque && rm -f *.spec && cblrpm spec abstract-deque-0.3
	spec-cleaner -i $@

